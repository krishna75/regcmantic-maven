  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_12.4">
    <isSubTopicOf rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_12.42"/>
