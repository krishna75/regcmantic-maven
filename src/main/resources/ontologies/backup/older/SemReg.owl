  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_4.3">
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4.3  Water</title>
    <isSubTopicOf rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_4.33"/>
    <hasTopic rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_4"/>
