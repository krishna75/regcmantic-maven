  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_11.5">
    <rdf:type rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#SubTopic"/>
