  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_7.3">
    <rdf:type rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#SubTopic"/>
    <isSubTopicOf rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_7.35"/>
