  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_4.1">
    <rdf:type rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#SubTopic"/>
