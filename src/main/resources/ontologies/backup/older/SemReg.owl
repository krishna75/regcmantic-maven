    <rdf:rest rdf:nodeID="A11"/>
    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">strong</rdf:first>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex_8.51_S1">
    <rdfs:comment>8.51  Production operations should be conducted in a manner that will prevent 
contamination of intermediates or APIs by other materials.</rdfs:comment>
