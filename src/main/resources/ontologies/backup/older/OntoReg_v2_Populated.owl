                    >employee</sp:varName>
                  </rdf:Description>
                </sp:object>
                <sp:predicate rdf:resource="#isResponsibilityOf"/>
                <sp:subject rdf:resource="http://spinrdf.org/spin#_this"/>
              </rdf:Description>
            </sp:elements>
          </sp:Optional>
          <sp:Filter>
            <sp:expression>
              <sp:bound>
                <sp:arg1 rdf:nodeID="A0"/>
              </sp:bound>
            </sp:expression>
          </sp:Filter>
        </sp:where>
      </sp:Ask>
