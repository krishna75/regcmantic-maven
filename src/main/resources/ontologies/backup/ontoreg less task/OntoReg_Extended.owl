    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <swrl:body>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:classPredicate rdf:resource="#Action"/>
            <swrl:argument1>
              <swrl:Variable rdf:ID="act"/>
            </swrl:argument1>
          </swrl:ClassAtom>
        </rdf:first>
        <rdf:rest>
          <swrl:AtomList>
            <rdf:first>
              <swrl:ClassAtom>
                <swrl:classPredicate rdf:resource="#Position"/>
                <swrl:argument1>
