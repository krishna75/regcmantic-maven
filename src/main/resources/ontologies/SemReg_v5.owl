  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex2007_18.16">
    <hasStatement rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex2007_18.16_2"/>
