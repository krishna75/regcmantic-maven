  <rdf:Description rdf:about="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex2007_6.41">
    <hasStatement rdf:resource="http://www.owl-ontologies.com/SemanticRegulation.owl#Eudralex2007_6.41_6"/>
